{
  "plugins": ["prettier"],
  "extends" : ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier/react", "eslint-config-prettier"], 
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true}],
    "import/first" : 0
  },
  "env": { 
    "browser": true,
    "node": true,
    "es6": true
  },

  "parser": "@babel/eslint-parser"
}