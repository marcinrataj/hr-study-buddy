{
  "plugins": ["prettier"],
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier", "eslint-config-prettier"],
  "rules": {
    "prettier/prettier": ["warn", {
      "singleQuote": true,
      "printWidth": 150
    }],
    "import/first": 0
  },
  
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "@babel/eslint-parser"
}